{
  "name": "logbin",
  "version": "1.3.4",
  "description": "NodeJS client for logbin.io",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "build-test": "rm -rf test-build && babel test --out-dir test-build",
    "build-all": "npm run build && npm run build-test",
    "jshint": "jshint src/ test/",
    "jscs": "jscs src/ test/",
    "jslint": "npm run jshint && npm run jscs",
    "test": "mocha -b --recursive test-build/",
    "prepublish": "npm run build-all"
  },
  "files": [
    "dist",
    "test-build",
    "logbin-config.json",
    "src",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logbin/logbin.git"
  },
  "keywords": [
    "logger",
    "log"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/logbin/logbin/issues"
  },
  "homepage": "https://github.com/logbin/logbin#readme",
  "jshintConfig": {
    "node": true,
    "undef": true,
    "unused": true,
    "esnext": true,
    "sub": true,
    "validthis": true,
    "noyield": true,
    "strict": true,
    "globals": {
      "describe": true,
      "it": true,
      "before": true,
      "after": true
    }
  },
  "jscsConfig": {
    "preset": "jquery",
    "fileExtensions": [
      "js"
    ],
    "requireCurlyBraces": true,
    "requireSemicolons": true,
    "requireSpacesInFunctionDeclaration": {
      "beforeOpeningRoundBrace": true,
      "beforeOpeningCurlyBrace": true
    },
    "disallowQuotedKeysInObjects": {
      "allExcept": [
        "reserved"
      ]
    },
    "validateIndentation": 2,
    "maximumLineLength": 125,
    "validateLineBreaks": "LF",
    "disallowTrailingComma": true,
    "disallowUnusedParams": true,
    "disallowImplicitTypeConversion": [
      "string"
    ],
    "validateQuoteMarks": "'",
    "esnext": true
  },
  "dependencies": {
    "bluebird": "^3.3.5",
    "co": "^4.6.0",
    "dateformat": "^1.0.12",
    "fs-promise": "^0.5.0",
    "lodash": "^4.8.2",
    "minimist": "^1.2.0",
    "node-cache": "^3.2.1",
    "node-uuid": "^1.4.7",
    "prettyjson": "^1.1.3",
    "babel-polyfill": "^6.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "co-mocha": "^1.1.2",
    "jscs": "^2.11.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5"
  },
  "babel": {
    "presets": [
      "babel-polyfill",
      "babel-preset-es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ],
    "retainLines": true
  }
}
